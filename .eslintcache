 = await api.get(`user/ecb/lt?estado=${estado}&cidade=${cidade}&bairro=${bairro}&id=${id}`)\r\n            }\r\n            else if(estado !== \"\" && cidade !== \"\") {\r\n                response = await api.get(`user/ec/lt?estado=${estado}&cidade=${cidade}&id=${id}`)\r\n            }\r\n            else if(estado !== \"\") {\r\n                response = await api.get(`user/e/lt?estado=${estado}&id=${id}`)\r\n            }\r\n    \r\n            setProfiles(response.data);\r\n        }\r\n        takeProfiles();\r\n    }, []);\r\n\r\n    return (\r\n        <div>\r\n            <Menu content={ContentHeaderLogado}/>\r\n\r\n            <div className=\"results-container\">\r\n                {profiles.length === 0 && (\r\n                    <div className='greenBack'>\r\n                        <div className='containerNoResults'>\r\n                            NÃO HÁ LARES TEMPORÁRIOS <br/> NESSA REGIÃO\r\n                        </div>\r\n                    </div>\r\n                )}\r\n                {profiles.length !== 0 && (\r\n                    <ul>\r\n                        {profiles.map(profile =>\r\n                            <Card profile={profile} key={profile.idUser}/>\r\n                        )}\r\n                    </ul>\r\n                )}\r\n            </div>\r\n\r\n            <Rodape content={ContentRodape1}/>\r\n            <Rodape2/>\r\n        </div>\r\n    );\r\n}\r\n","C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\pages\\Update\\index.js",["228"],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\pages\\OwnProfile\\index.js",["229","230","231","232"],"import React, {useEffect, useState} from 'react';\r\nimport { Link, useHistory } from \"react-router-dom\";\r\nimport { FiChevronLeft, FiChevronRight } from \"react-icons/fi\";\r\nimport api from '../../Services/api';\r\nimport './style.css';\r\n\r\nimport Menu from '../../templates/Menu';\r\nimport ContentHeaderLogado from '../../templa