essages":"219","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\index.js",[],["220","221"],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\App.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\routes.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\pages\\Register\\index.js",["222","223"],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\pages\\TelaPrincipal\\index.js",["224"],"import React, {useEffect, useState} from 'react';\r\nimport { useHistory } from \"react-router-dom\";\r\nimport { FiSearch } from \"react-icons/fi\";\r\nimport './style.css';\r\n\r\nimport Menu from '../../templates/Menu';\r\nimport ContentHomeAboutContact from '../../templates/ContentHomeAboutContact';\r\nimport Rodape from '../../templates/Rodape';\r\nimport ContentRodape1 from '../../templates/ContentRodape1';\r\nimport Rodape2 from '../../templates/Rodape2';\r\n\r\nexport default function Home() {\r\n    const [estado, setEstado] = useState('');\r\n    const [cidade, setCidade] = useState('');\r\n    const [bairro, setBairro] = useState('');\r\n\r\n    const id = localStorage.getItem('Token');\r\n    const history = useHistory();\r\n\r\n    useEffect (() => {\r\n        if(!id) {\r\n            history.push('/error');\r\n        }\r\n    },   [])\r\n\r\n    async function pesquisa(e) {\r\n        e.preventDefault();\r\n\r\n        try {\r\n            localStorage.setItem('estado', estado);\r\n            localStorage.setItem('cidade', cidade);\r\n            localStorage.setItem('bairro', bairro);\r\n            history.push('/resultLT');\r\n        }\r\n        catch (err) {\r\n            alert('Erro na pesquisa. Tente novamente.')\r\n        }\r\n    };\r\n\r\n    async function deslogar () {\r\n        localStorage.clear();\r\n        history.push('/');\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <Menu content={ContentHomeAboutContact}/>\r\n            \r\n            <div className=\"home-container\">\r\n                <div className=\"img\">\r\n                    <div className=\"itensImg\">\r\n                        <p>Encontre um novo <br/> amigo para te ajudar <br/> nesse resgate animal</p>\r\n                        <button className=\"loginButton\" onClick={deslogar}>Logout</button>\r\n                    </div>\r\n\r\n                    <div className=\"formAlign\">\r\n                        <form method=\"post\" onSubmit={pesquisa}>\r\n                            <input type=\"text\" placeholder=\"UF (estado)\" maxLength='2' required pattern='[A-Z]{2}' title='Apenas Letras maiúsculas'\r\n                            value={estado}\r\n                            onChange={e => setEstado(e.target.value)}></input>\r\n\r\n                            <input type=\"text\" placeholder=\"CIDADE\" maxLength='40'\r\n                            value={cidade}\r\n                            onChange={e => setCidade(e.target.value)}></input>\r\n\r\n                            <input type=\"text\" placeholder=\"BAIRRO\" maxLength='40' \r\n                            value={bairro}\r\n                            onChange={e => setBairro(e.target.value)}></input>\r\n\r\n                            <button type=\"submit\" value=\"submit\" ><FiSearch size={50} className='iconButton'/></button>\r\n                        </form>\r\n                    </div>\r\n                    \r\n                    <div className=\"blankSpace\">aaaaaaaaaaa</div>\r\n                </div>\r\n                <div className=\"imgPatas\">\r\n                    <div className=\"contentAbout\">\r\n                            <p className=\"sobre\" id=\"about\">SOBRE</p><br/>\r\n                            <div className='linhas'>\r\n                                <hr className=\"linha\"/> \r\n                                <hr className=\"linha\"/> \r\n                            </div>\r\n\r\n                            <div className='arrumar'>\r\n                                <p className='prim'> O Ephemeral é uma plataforma desenvolvida através de um \r\n                                                     trabalho de conclusão realizado no Instituto Federal do  Rio \r\n                                                     Grande do Sul. \r\n                                                     \r\n                                                     <br/><br/> \r\n\r\n                                                     Tem o intuito de auxiliar voluntários à lar temporário a \r\n                                                     acharem mais rapidamente um resgatador de animais, \r\n                                                     facilitando o processo de encontro entre ambos. </p>\r\n                            </div>\r\n                    </div>\r\n                </div>\r\n                \r\n                <Rodape content={ContentRodape1}/>\r\n                <Rodape2/>\r\n            </div>  \r\n        </div>\r\n    );\r\n}","C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\pages\\Login\\index.js",["225"],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\templates\\Menu\\index.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\templates\\ContentHomeHelp\\index.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\templates\\ContentHomeAboutContact\\index.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\templates\\Rodape\\index.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\templates\\ContentRodape1\\index.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\templates\\Rodape2\\index.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\Services\\api.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\templates\\ContentHeaderLogado\\index.js",[],"C:\\Users\\kamil\\OneDrive\\Documentos\\tccEphemeral\\frontend\\src\\pages\\Help\\index.js",["226"],"import React, { useEffect, useState } from 'react';\r\nimport './style.css';\r\n\r\nimport Menu from '../../templates/Menu';\r\nimport ContentHomeHelp from '../../templates/ContentHomeHelp';\r\nimport ContentHeaderLogado from '../../templates/ContentHeaderLogado';\r\nimport Rodape from '../../templates/Rodape';\r\nimport ContentRodape1 from '../../templates/ContentRodape1';\r\nimport Rodape2 from '../../templates/Rodape2';\r\n\r\nexport default function Help() {\r\n    const token = localStorage.getItem('Token');\r\n    const [verifica, setVerifica] = useState('');\r\n\r\n    useEffect (() => {\r\n        if(token) {\r\n            setVerifica(true);\r\n        }\r\n        else {\r\n            setVerifica(false);\r\n        }\r\n    }, [])\r\n\r\n    return (\r\n        <div>\r\n            {verifica === true && (\r\n                <Menu content={ContentHeaderLogado}/>\r\n            )}\r\n            {verifica === false && (\r\n                <Menu content={ContentHomeHelp}/>\r\n            )}\r\n\r\n            <div className=\"containerPrincipal\">\r\n                <div className=\"dicas-container\">\r\n                    <p>\r\n                        <b>O que é um lar temporário (LT)?</b> Um lar temporário disponibiliza sua casa para que cães e gatos abandonados <br/> \r\n                        tenham um abrigo temporário, alimentação e cuidados, até que sejam adotados por uma família definitiva.\r\n                    </p>\r\n\r\n                    <p>\r\n                        <b>Quais os atores do sistem?</b> Temos dois usuários: os responsáveis pelo animal (entidade individual ou ONG) e <br/> os voluntários a lar temporário. \r\n                    </p>\r\n\r\n                    <p>\r\n                        <b>Como funciona a participação na plataforma para cada um